{
  "extends": "@vook-client/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/providers/*": ["./src/providers/*"],
      "@/components/*": ["./src/components/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/mock/*": ["./src/mock/*"],
      "@/modals/*": ["./src/modals/*"],
      "@/toasters/*": ["./src/toasters/*"],
      "@/store/*": ["./src/store/*"]
    },
    "types": ["chrome"]
  },
  "include": [
    "next-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/(onboarding)/onboarding/funnel/fsafs"
  ],
  "exclude": ["node_modules"]
}
